<%- include("partials/header") %>

<h1><%= homeContentTitle %></h1>
<p><%= homeContentBody %></p>

<div> 
     
    <% submittedPost.forEach(function (post){ %>
        <div>
            <h3><%= post.contentTitle %></h3>
            <p><%= post.contentBody.substring(0, 100); %> ... <a href="/posts/<%= post.contentTitle %>" >Read More</a> </p>
        </div>
    <% }) %>

</div>

<%- include("partials/footer") %>